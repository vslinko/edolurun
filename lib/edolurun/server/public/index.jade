doctype 5
html(lang="en", ng-app="edolurun")
  head
    title Edolurun

    script(src="//cdnjs.cloudflare.com/ajax/libs/jquery/1.8.3/jquery.min.js")
    script(src="//cdnjs.cloudflare.com/ajax/libs/angular.js/1.1.1/angular.min.js")
    script(src="//cdnjs.cloudflare.com/ajax/libs/jq-console/2.7.7/jqconsole.min.js")

    script(src="/scripts/index.js")
    script(src="/scripts/modules/jqconsole.js")

    link(rel="stylesheet", href="/styles/jqconsole.css")

  body(ng-controller="EdolurunCtrl")
    #console(jqconsole, welcome-string="Welcome to Edolurun!", on-input="consoleInput(input, jqconsole, callback)")
